{
    "name": "camelot/collection",
    "description": "Object Orientated implementations of various collections",
    "license": "MIT",
    "authors": [
        {
            "name": "Carson Full",
            "email": "carsonfull@gmail.com"
        },
        {
            "name": "Gawain Lynch",
            "email": "gawain.lynch@gmail.com"
        }
    ],
    "require": {
        "camelot/common": "^1.0",
        "ext-json": "*",
        "php": "^7.1.3",
        "symfony/polyfill-php71": "^1.3",
        "webmozart/assert": "^1.0"
    },
    "require-dev": {
        "camelot/coding-style": "^1.0",
        "escapestudios/symfony2-coding-standard": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpunit/phpunit": "^7.3",
        "symfony/phpunit-bridge": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Camelot\\Collection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Camelot\\Collection\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": [
            "phpcbf",
            "php-cs-fixer fix"
        ]
    }
}
