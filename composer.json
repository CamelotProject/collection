{
    "name": "camelot/collection",
    "description": "Object Orientated implementations of various collections",
    "license": "MIT",
    "authors": [
        {
            "name": "Carson Full",
            "email": "carsonfull@gmail.com"
        },
        {
            "name": "Gawain Lynch",
            "email": "gawain.lynch@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3 || ^8.0",
        "camelot/common": "^1.0",
        "ext-json": "*",
        "symfony/polyfill-php71": "^1.3",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "camelot/coding-style": "^3.0",
        "escapestudios/symfony2-coding-standard": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Camelot\\Collection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Camelot\\Collection\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": [
            "phpcbf",
            "php-cs-fixer fix"
        ]
    }
}
